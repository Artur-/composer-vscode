{
  "compilerOptions": {
    "sourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "outDir": "out"
  },
  "include": ["ext-src/**/*.ts"],
  "exclude": []
}
